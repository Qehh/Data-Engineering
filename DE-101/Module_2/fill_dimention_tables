INSERT INTO calendar_dim
SELECT 
order_date,
EXTRACT(YEAR from order_date),
EXTRACT(QUARTER from order_date),
EXTRACT(MONTH from order_date),
EXTRACT(WEEK from order_date),
EXTRACT(DAY from order_date)
FROM orders;

INSERT INTO geographic_dim
SELECT 
(100+row_number() over ()),
country,
region,
state,
city,
postal_code,
orders.row_id
FROM orders;

INSERT INTO customer_dim
SELECT 
customer_id,
customer_name,
segment,
row_id
FROM orders;

INSERT INTO order_dim
SELECT 
order_id,
order_date,
ship_date,
row_id,
ship_mode
FROM orders;

INSERT INTO product_dim
SELECT 
product_id,
product_name,
category,
subcategory,
row_id
FROM orders;

INSERT INTO sales_fact
SELECT 
row_id,
sales,
profit,
order_date,
discount,
quantity
FROM orders;
